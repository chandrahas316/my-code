{
    "version": 2,
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      }
    ],
    "env": {
      "TESSDATA_PREFIX": "/app/.apt/usr/share/tesseract-ocr/4.00/tessdata"
    }
  }
